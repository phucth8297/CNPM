<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:SDKBrowser.Examples.ChartControl.PieChartCategory.PieChartExample"
             xmlns:telerikChart="clr-namespace:Telerik.XamarinForms.Chart;assembly=Telerik.XamarinForms.Chart"
             x:Class="SDKBrowser.Examples.ChartControl.PieChartCategory.DonutSeriesExample.DonutSeriesXAML">
    <!-- >> chart-series-donut-xaml -->
    <telerikChart:RadPieChart>
        <telerikChart:RadPieChart.BindingContext>
            <local:ViewModel />
        </telerikChart:RadPieChart.BindingContext>
        <telerikChart:RadPieChart.Series>
            <telerikChart:DonutSeries ShowLabels="True"
                                      InnerRadiusFactor="0.4"
                                      ValueBinding="Value"
                                      ItemsSource="{Binding Data}" />
        </telerikChart:RadPieChart.Series>
    </telerikChart:RadPieChart>
    <!-- << chart-series-donut-xaml -->
</ContentView>
